<UserControl x:Class="PA3UI.ui.CardHolder"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:PA3UI.ui"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <ScrollViewer>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="1*"/>
                <RowDefinition Height="1*"/>
                <RowDefinition Height="1*"/>
                <RowDefinition Height="1*"/>
            </Grid.RowDefinitions>
            
            
            <Image Grid.Row="0" Source="../Assets/Deeds/Boardwalk.png"></Image>
            <Image Grid.Row="1" Source="../Assets/Deeds/ConnecticutAve.png"></Image>
            <Image Grid.Row="2" Source="../Assets/Deeds/ReadingRailroad.png"></Image>
            <Image Grid.Row="3" Source="../Assets/Deeds/AtlanticAve.png"></Image>
            
            
        </Grid>
    </ScrollViewer>
</UserControl>
